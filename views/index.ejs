    <script type="text/javascript">

      $("#ImageSubmissionForm").submit(function (e) {
      e.preventDefault();
      var formData = new FormData(this);
      $.ajax({
          type: "POST",
          url: "/submit",
          data: formData,
          processData: false,
          contentType: false,
          success: function(r){
              $.ajax({
                type: "POST",
                url: "/metadata",
                data: {path: r},
                success: function(r){
                  setTimeout(function(){
                    $('.downloadButton').click();
                  }, 2000)
                  console.log("result 2",r.exif.ApertureValue)
                  $(".data").html(" ")
                    $('.card-text').html(' ')
                    if (Object.keys(r.exif).length > 0) {
                      $(".data").append(
                      '<p style="background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px"> ApertureValue: ' + r.exif.ApertureValue + '</p>',
                      '<p style="background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;"> Color Space: ' + r.exif.ColorSpace + '</p>',
                      '<p style="background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;"> CustomRendered: ' + r.exif.CustomRendered + '</p>',
                      '<p style="background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;"> DateTimeOriginal: ' + r.exif.DateTimeOriginal + '</p>',
                      '<p style="background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;"> ExifImageHeight: ' + r.exif.ExifImageHeight + '</p>',
                      '<p style="background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;"> ExifImageWidth: ' + r.exif.ExifImageWidth + '</p>',
                      '<p style="background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;"> ExposureCompensation: ' + r.exif.ExposureCompensation + '</p>',
                      '<p style="background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;"> ExposureMode: ' + r.exif.ExposureMode + '</p>',
                      '<p style="background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;"> ExposureProgram: ' + r.exif.ExposureProgram + '</p>',
                      '<p style="background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;"> ExposureTime: ' + r.exif.ExposureTime + '</p>',
                      '<p style="background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;"> FNumber: ' + r.exif.FNumber + '</p>',
                      '<p style="background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;"> Flash: ' + r.exif.Flash + '</p>',
                      '<p style="background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;"> FocalLength: ' + r.exif.FocalLength + '</p>',
