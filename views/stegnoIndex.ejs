  <script type="text/javascript">

    $("#ImageSubmissionForm").submit(function (e) {
      e.preventDefault();
      var formData = new FormData(this);
      $.ajax({
        type: "POST",
        url: "/submit",
        data: formData,
        processData: false,
        contentType: false,
        success: function (r) {
          $.ajax({
            type: "POST",
            url: "/stegnography",
            data: { path: r, script: "binwalk" },
            success: function (r) {
              $.ajax({
                type: "POST",
                url: "/stegnofinder",
                data: { path: r },
                success: function (r) {
                  $('.loader').css({display: 'block'});
                  $('.covers').css({display: 'block'});
                  $('body').css({background: 'none'});
                  // $('div#to-hide').css({display: 'none'});
                  setTimeout(function(){
                    const output = r.file.map(cur => ({
                    file: cur.file,
                    text: r.text ? r.text : null,
                  }))
                  $('.imageButton').html(' ')
                  if (r.file.length > 0) {
                    $.each(output, function(key, value){
                      console.log(window.location.origin +'/'+value.file)
                      $(".imageButton").append(
                        `<a style="margin-top:20px" class="btn btn-primary" href=${window.location.origin +'/'+value.file} download>Download Embedded File</a> <br>`
                      )
                    });
                  } else {
                    $('.imageButton').append(
                      '<p style="color: red;background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;">' + 'Sorry No Data Found' + '</p>',
                    )
                  }

                  $('.imageText').html(' ')
                  if (r.text) {
                    console.log('hello')
                    $('.imageText').append(
                      `<p>${r.text}</p>`
                    )
                  } else {
                    $('.imageText').append(
                      '<p style="color: red;background-color: #f9f9f9; border-top: 1px solid #dee2e6; border-bottom: 1px solid #dee2e6; padding:10px; border-top: 1px solid #dee2e6;">' + 'Sorry No Data Found' + '</p>',
                    )
                  }

                  $('.loader').css({display: 'none'});
                  $('.covers').css({display: 'none'});
                  $('div#to-hide').css({display: 'block'});
                  $('body').css({background:
                    'linear-gradient(rgba(0, 0, 0, 0.35), rgba(0, 0, 0, 0.35)), url(https://www.logicforce.com/wp-content/uploads/2019/03/iStock-465716918-1.jpg) no-repeat',
                    backgroundSize: 'cover'
                   });
                  }, 3000)
                },
                error: function (e) {
                  $('')
                }
              });
            },
            error: function (e) {
              console.log("some error", e);
            }
          });
        },
